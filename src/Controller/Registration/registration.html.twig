{% extends 'base.html.twig' %}

{% block title %}Registration{% endblock %}

{% block body %}
    <div class="page-header">
        <h1>Registration</h1>
    </div>

    {{ form_start(form) }}
    <div class="form-group">
        {{ form_row(form.email) }}
    </div>
    <div class="form-group">
        {{ form_row(form.username) }}
    </div>
    <div class="form-group">
        {{ form_row(form.password) }}
    </div>
    {{ form_end(form) }}
{% endblock %}
